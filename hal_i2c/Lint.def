  NBFUNC {level=2}  // "Non-blocking assignment encountered in function '%s'"
  BADSYS {level=2}  // "System task '%s' in module '%s' is ignored" 
  RTLNOG {level=2}  // "Gate instances are not expected in an RTL design"
